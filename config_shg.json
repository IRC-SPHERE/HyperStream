{
    "process": {
        "environmental": false,
        "video": false,
        "wearable": false,
        "annotations": false
    },
    "process_on_startup": false,
    "mongo": {
        "host": "localhost",
        "port": 27021,
        "tz_aware": true,
        "db": "sphere",
        "modalities": {
            "environmental": {
                "collection": "wp1all",
                "date_time_field": ["dt", "bt"],
                "fields": {
                    "dt": 1,
                    "bt": 1,
                    "uid": 1,
                    "e": 1,
                    "m": 1,
                    "n": 1,
                    "s": 1,
                    "sn": 1
                }
            },
            "wearable": {
                "collection": "WEARABLE-ISO-TIME",
                "date_time_field": "wts",
                "event": "m",
                "fields": {
                    "wts": 1,
                    "uid": 1,
                    "aid": 1,
                    "m": 1,
                    "rss": 1
                }
            },
            "wearable2": {
                "collection": "wearable",
                "date_time_field": "bt",
                "event": "e",
                "fields": {
                    "gw": 1,
                    "uid": 1,
                    "e": 1,
                    "bt": 1
                }
            },
            "wearable3": {
                "collection": "wearable",
                "date_time_field": "bt",
                "event": "e",
                "fields": {
                    "gw": 1,
                    "uid": 1,
                    "e": 1,
                    "bt": 1
                }
            },
            "wearable4": {
                "collection": "WEARABLE-ISO-TIME",
                "date_time_field": "wts",
                "event": "m",
                "fields": {
                    "wts": 1,
                    "uid": 1,
                    "aid": 1,
                    "m": 1,
                    "rss": 1
                }
            },
            "video": {
                "collection": "VIDEO",
                "date_time_field": "dt",
                "event": "e",
                "element_name": "n",
                "fields": {
                    "dt": 1,
                    "e": 1,
                    "uid": 1,
                    "n": 1
                }
            },
            "annotations": {
                "collection": "annotations"
            }
        }
    }
}
