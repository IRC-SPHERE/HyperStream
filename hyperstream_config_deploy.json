{
    "mongo": {
        "host": "localhost",
        "port": 27017,
        "tz_aware": true,
        "db": "hyperstream"
    },
    "plugins": [
    {
        "channel_id_prefix": "sphere",
        "path": "plugins/sphere",
        "channel_names": ["sphere_channel"],
        "has_tools": true
    }],
    "online_engine": {
        "interval": {
            "start": -60,
            "end": -10
        },
        "sleep": 5,
        "iterations": 100
    }
}
